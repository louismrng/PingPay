{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "PingPay": "Information"
      }
    }
  },
  "Solana": {
    "UseDevnet": false,
    "Commitment": "confirmed"
  },
  "Telegram": {
    "BotToken": "",
    "WebhookUrl": "",
    "UsePolling": false
  },
  "Signal": {
    "PhoneNumber": "",
    "SignalCliPath": "/usr/bin/signal-cli",
    "ApiEndpoint": ""
  },
  "KeyManagement": {
    "Provider": "AzureKeyVault"
  }
}
